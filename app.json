{
  "expo": {
    "name": "justMySizeApp",
    "slug": "justMySizeApp",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": {
      "image": "./assets/images/jms_icon.png",
      "backgroundColor": "#6674DE"
    },
    "scheme": "justmysize",
    "splash": {
      "image": "./assets/images/jms_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6674DE"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/images/jms_icon.png"
    }
  }
}
